<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD GWT 2.7.0//EN"
    "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='AppModTest'>
  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Other module inherits -->
  <inherits name='com.sencha.gxt.ui.GXT' />

  <!-- GXT Normalizing Stylesheet -->
  <stylesheet src="reset.css" />

  <!-- GXT Theme -->
  <!-- <inherits name='com.sencha.gxt.theme.blue.Blue'/> -->
  <!-- <inherits name='com.sencha.gxt.theme.gray.Gray' /> -->
  <inherits name="com.sencha.gxt.theme.neptune.Theme" />

  <!-- Specify the app entry point class. -->
  <entry-point class='it.pkg.client.AppModTestEntryPoint' />

  <!-- Specify the paths for translatable code -->
  <source path='client' />
  <source path='shared' />
  
  <!-- http://docs.sencha.com/gxt/3.1/getting_started/sdm/SuperDevMode.html -->
  <!-- Use this when GWT < 2.7 -->
  <!-- <add-linker name="xsiframe" /> -->
  
</module>
